<fieldset>

  <div class="field">Applicant:
    <%= f.text_field :ap_name %>
  </div>
  <div class="field">Address Line 1:
    <%= f.text_field :ap_address_1 %>
  </div>
  <div class="field">Address Line 2:
    <%= f.text_field :ap_address_2 %>
  </div>
  <div class="field">City, State Zip:
    <%= f.text_field :ap_city_state_zip %>
  </div>
  <input type="button" name="previous" class="previous action-button" value="Previous" />
      <input type="button" name="next" class="next action-button" value="Next" />
</fieldset>
